{
    "name": "adv",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@apollo/client": "^3.2.3",
        "@auth0/auth0-react": "^1.1.0",
        "apollo-server-lambda": "^2.18.2",
        "dotenv": "^8.2.0",
        "graphql": "^15.3.0",
        "graphql-tools": "^6.2.4",
        "history": "^5.0.0",
        "mongodb": "^3.6.1",
        "mongodb-client-encryption": "^1.1.0",
        "react": "^16.13.1",
        "react-bootstrap": "^1.3.0",
        "react-day-picker": "^7.4.8",
        "react-dom": "^16.13.1",
        "react-router-dom": "^5.2.0"
    },
    "scripts": {
        "start": "NODE_ENV=development npm-run-all --parallel start:api start:client",
        "start:client": "react-scripts start",
        "start:api": "netlify-lambda serve functions",
        "build": "npm-run-all --parallel build:prod build:functions",
        "build:prod": "node ./scripts/create-env.js; react-scripts build; cp _redirects build/",
        "build:functions": "netlify-lambda build functions",
        "generate-graphql": "graphql-codegen --config codegen.yml"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@graphql-codegen/cli": "1.17.10",
        "@graphql-codegen/typescript-mongodb": "1.17.8",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/auth0-js": "^9.14.0",
        "@types/jest": "^24.0.0",
        "@types/mongodb": "^3.5.27",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.0",
        "@types/react-dom": "^16.9.0",
        "@types/react-router-dom": "^5.1.5",
        "cross-env": "^7.0.2",
        "netlify-cli": "^2.61.2",
        "netlify-lambda": "^2.0.1",
        "npm-run-all": "^4.1.5",
        "react-scripts": "3.4.3",
        "typescript": "~3.7.2",
        "@graphql-codegen/typescript": "1.17.10",
        "@graphql-codegen/typescript-operations": "1.17.8",
        "@graphql-codegen/typescript-resolvers": "1.17.10",
        "@graphql-codegen/typescript-react-apollo": "2.0.7",
        "@graphql-codegen/introspection": "1.18.0"
    },
    "proxy": "http://localhost:9000"
}